<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="com.thinkgem.jeesite.autocode.actionSet">
         <menu
               label="JEESITE"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="增删改查模块生成"
               icon="icons/curd.png"
               class="com.thinkgem.jeesite.autocode.actions.SampleAction"
               tooltip="增删改查模块"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="com.thinkgem.jeesite.autocode.actions.SampleAction">
         </action>
         <action
               class="com.thinkgem.jeesite.autocode.actions.DataSourceAction"
               icon="icons/database.png"
               id="com.thinkgem.jeesite.autocode.actions.DataSourceAction"
               label="数据源管理"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               tooltip="数据源管理">
         </action>
      </actionSet>
   </extension>

</plugin>
